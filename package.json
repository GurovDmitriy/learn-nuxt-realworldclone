{
  "name": "start-repository-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "npm run lint && webpack --mode production",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "jsonserver:dynamic": "json-server --watch ./data/db.js -m ./node_modules/json-server-auth -r routes.json",
    "jsonserver:static": "json-server --watch ./data/db.json -m ./node_modules/json-server-auth -r routes.json",
    "fakedb": "node ./fakedb.js && prettier --write \"./data/db.json\"",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "lint:eslint": "eslint --fix --ext \".vue,.js\" --ignore-path .eslintignore .",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,css,less,scss,html}\" --ignore-path .stylelintignore",
    "lint:prettier": "prettier --write \"**/*.{vue,js,css,less,scss,html,md}\" --ignore-path .prettierignore",
    "lint:prepare": "husky install"
  },
  "lint-staged": {
    "*.{vue,js}": "eslint --fix",
    "*.{vue,css,less,scss,html}": "stylelint --fix",
    "*.{vue,js,css,less,scss,html,md}": "prettier --write"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/svg-sprite": "^0.5.2",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.20.3",
    "date-fns": "^2.28.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.15.7",
    "stylelint-config-prettier": "^9.0.3"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.4.0",
    "@babel/eslint-parser": "^7.16.3",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxtjs/eslint-config": "^7.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "casual": "^1.6.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "json-server": "^0.17.0",
    "json-server-auth": "^2.1.0",
    "lint-staged": "^12.1.2",
    "lqip-loader": "^2.2.1",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.0",
    "responsive-loader": "^2.3.0",
    "sass": "^1.43.5",
    "sass-loader": "^10.1.1",
    "sharp": "^0.30.1",
    "sqip-loader": "^1.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-order": "^5.0.0",
    "webp-loader": "^0.6.0"
  }
}
